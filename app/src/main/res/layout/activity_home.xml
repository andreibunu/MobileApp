<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

    <!--        tools:layout_editor_absoluteX="8dp"-->
    <!--        tools:layout_editor_absoluteY="181dp" />-->

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Visit a room"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline25"
        app:layout_constraintEnd_toStartOf="@+id/vguideline75"
        app:layout_constraintHorizontal_bias="0.477"
        app:layout_constraintStart_toStartOf="@+id/vguideline25"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.068" />

    <ImageButton
        android:id="@+id/livingroom"
        android:layout_width="180dp"
        android:layout_height="180dp"

        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="148dp"
        android:background="@android:color/transparent"
        android:contentDescription="Living room"
        android:maxWidth="36dp"
        android:maxHeight="36dp"
        android:onClick="visitRoom"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/bedroom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline0"
        app:srcCompat="@drawable/livingroom" />

    <ImageButton
        android:id="@+id/bedroom"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginTop="148dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:background="@android:color/transparent"
        android:contentDescription="Bedroom"
        android:maxWidth="36dp"
        android:maxHeight="36dp"
        android:onClick="visitRoom"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/vguideline1"
        app:layout_constraintStart_toEndOf="@+id/livingroom"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bedroom" />

    <ImageButton
        android:id="@+id/kitchen"
        android:layout_width="180dp"
        android:layout_height="180dp"

        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="148dp"
        android:background="@android:color/transparent"
        android:contentDescription="Kitchen"
        android:maxWidth="36dp"
        android:maxHeight="36dp"
        android:onClick="visitRoom"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/bathroom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline25"
        app:srcCompat="@drawable/kitchen" />
    />

    <ImageButton
        android:id="@+id/bathroom"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginTop="148dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:background="@android:color/transparent"
        android:contentDescription="Bathroom"
        android:maxWidth="36dp"
        android:maxHeight="36dp"
        android:onClick="visitRoom"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/vguideline1"
        app:layout_constraintStart_toEndOf="@+id/kitchen"
        app:layout_constraintTop_toTopOf="@+id/guideline25"
        app:srcCompat="@drawable/bathroom" />
    />

    <ImageButton
        android:id="@+id/basement"
        android:layout_width="140dp"
        android:layout_height="140dp"

        android:layout_marginTop="152dp"
        android:background="@android:color/transparent"
        android:contentDescription="Basement"
        android:maxWidth="30dp"
        android:maxHeight="30dp"
        android:onClick="visitRoom"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/guideline100"
        app:layout_constraintEnd_toStartOf="@+id/vguideline75"
        app:layout_constraintStart_toStartOf="@+id/vguideline25"
        app:layout_constraintTop_toTopOf="@+id/guideline50"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/base" />
    />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp"
        app:layout_constraintGuide_percent="0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp"
        app:layout_constraintGuide_percent="0.75" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp"
        app:layout_constraintGuide_percent="1.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguideline0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguideline25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.25"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguideline75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.75"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vguideline1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="1.0"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />


</androidx.constraintlayout.widget.ConstraintLayout>